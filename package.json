{
	"name": "discord.js-codemod",
	"version": "0.2.2",
	"bin": "./cli.js",
	"type": "commonjs",
	"private": false,
	"author": "Khasms",
	"description": "Discord.js codemod scripts.",
	"keywords": [
		"discord",
		"discord.js",
		"discordjs",
		"codemod",
		"jscodeshift"
	],
	"scripts": {
		"preinstall": "npx only-allow pnpm",
		"lint": "eslint --ext js,ts src/**/*.js",
		"lint:fix": "eslint --fix --ext js,ts src/**/*.js",
		"prepare": "is-ci || husky install",
		"prepublishOnly": "pnpm run lint && pnpm run test",
		"test": "jest --passWithNoTests",
		"test:ci": "jest --verbose --no-stack-trace",
		"release": "standard-version --preset angular"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Khasms/discord.js-codemod.git"
	},
	"license": "Apache-2.0",
	"bugs": {
		"url": "https://github.com/Khasms/discord.js-codemod/issues"
	},
	"homepage": "https://github.com/Khasms/discord.js-codemod#README",
	"dependencies": {
		"@typescript-eslint/typescript-estree": "^4.33.0",
		"jscodeshift": "^0.13.0",
		"typescript": "^4.4.3",
		"yargs": "^17.2.1"
	},
	"devDependencies": {
		"@commitlint/cli": "^13.2.0",
		"@commitlint/config-angular": "^13.2.0",
		"@types/jscodeshift": "^0.11.2",
		"@typescript-eslint/eslint-plugin": "^4.33.0",
		"@typescript-eslint/parser": "^4.33.0",
		"eslint": "^7.32.0",
		"eslint-config-marine": "^9.0.6",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^4.0.0",
		"husky": "^7.0.2",
		"is-ci": "^3.0.0",
		"jest": "^27.2.4",
		"lint-staged": "^11.2.0",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.4.1",
		"rimraf": "^3.0.2",
		"standard-version": "^9.3.1"
	}
}
